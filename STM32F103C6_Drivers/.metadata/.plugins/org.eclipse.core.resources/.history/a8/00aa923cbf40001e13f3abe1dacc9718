#include"I2C_EEPROM_SLAVE.h"
#include "I2C.h"

void EEPROM_Init(void){
	//I2C INIT
	//	PB6 = SCL
	//	PB7 = SDA

	I2C_Config_t I2C1CFG;

	// i2C ACT AS MASTER
	I2C1CFG.I2C_Acknowledge_Mode = I2C_ACK_RETURNED;
	I2C1CFG.I2C_Clock_Freq = I2C_SCLK_SM_100K;

}
